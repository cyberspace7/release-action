{
  "name": "@cyberspace7/release-action",
  "version": "0.6.1",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write \"**/*.{cjs,mdj,js,jsx,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,mdj,js,jsx,ts,tsx,md,astro}\"",
    "lint": "eslint --max-warnings 0 .",
    "check": "tsc --noEmit",
    "test": "jest",
    "build": "ncc build src/index.ts --source-map --license licenses.txt -o dist"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "@octokit/request-error": "5.0.1",
    "@octokit/rest": "20.0.2",
    "lodash-es": "4.17.21",
    "semver": "7.5.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@commitlint/types": "18.6.0",
    "@cyberspace-7/eslint-config": "0.1.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/eslint": "8.56.2",
    "@types/jest": "29.5.12",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.18",
    "@types/semver": "7.5.6",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.15.2",
  "engines": {
    "node": "^18.18"
  },
  "engineStrict": true
}
